@model IEnumerable<WebMusicaGrupoC.Models.Artistas>

@{
    ViewData["Title"] = "IndexListadoArtistasMayores";
}

<h1>Listado de Artistas que nacieron después de 1950</h1>

<p>
    <a asp-action="Index">Volver</a>
</p>
<table class="table table-striped">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Genero)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaNac)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Genero)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaNac)
            </td>

        </tr>
}
    </tbody>
</table>
